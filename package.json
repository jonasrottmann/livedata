{
  "name": "@jonasrottmann/livedata",
  "version": "0.0.8",
  "description": "✨ Simple, zero dependency, observable value container. ✨",
  "main": "dist/livedata.js",
  "module": "dist/livedata-module.js",
  "unpkg": "dist/livedata.min.js",
  "files": [
    "dist",
    "index.d.ts",
    "*.d.ts"
  ],
  "scripts": {
    "predoc": "rm -rf ./docs/",
    "doc": "typedoc --theme markdown --hideSources --hideBreadcrumbs --mode file --readme none --gitRevision master --includeDeclarations --excludeExternals --out ./docs/ ./*.d.ts;",
    "clean": "rm -rf dist/",
    "prebuild": "npm run clean",
    "build": "rollup -c",
    "postbuild": "npm run doc",
    "test": "run-p test-*",
    "test-unit": "nyc ava",
    "test-lint": "xo",
    "test-typings": "tsd"
  },
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonasrottmann/livedata.git"
  },
  "author": "Jonas Rottmann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jonasrottmann/livedata/issues"
  },
  "homepage": "https://github.com/jonasrottmann/livedata#readme",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "ava": "^2.4.0",
    "concat-md": "^0.3.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "rollup": "^1.26.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "sinon": "^7.5.0",
    "tsd": "^0.10.0",
    "typedoc": "^0.15.0",
    "typedoc-plugin-markdown": "^2.2.11",
    "xo": "^0.25.3"
  },
  "keywords": [
    "data flow",
    "observable",
    "reactive",
    "state container",
    "state management",
    "state store",
    "state",
    "store"
  ],
  "ava": {
    "require": [
      "@babel/register"
    ]
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "envs": [
      "shared-node-browser"
    ],
    "ignores": [
      "*.d.ts",
      "*.test-d.ts"
    ]
  }
}
